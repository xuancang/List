.log-list {
display: flex;
flex-direction: column;
padding: 0rpx;
}
.date {
float: left;
background: none repeat scroll 0 0 #FFEEEE;
width: 10%;
padding: 0 0px;
padding-left:0px;
height: 42px;
color: #25d415;
font-size: 16px; 
line-height: 42px;
border: 0px solid #E70047;
margin: 0rpx;
text-align: center;
}
.data{
float: left;
background: none repeat scroll 0 0 rgb(238, 255, 255);
width: 70%;
padding: 0 0px;
padding-left:0px;
height: 42px;
color: #db4a1e;
font-size: 16px; 
line-height: 42px;
border: 0px solid #E70047;
margin: 0rpx;
text-align: center;
}
.circle {
border-radius: 50%;
float: left;
background: none repeat scroll 0 0 rgb(243, 34, 34);
width: 1rem;
height: 1rem;
padding: 0 0px;
padding-left:0px;
color: #db4a1e;
font-size: 16px; 
line-height: 42px;
border: 0px solid #E70047;
margin: 0rpx;
text-align: center;
}
button{
display: inline-block;
vertical-align: baseline;
margin: 0 2px;
margin-top:30rpx;
outline: none;
text-align: center;
text-decoration: none;
font: 14px/100% Arial, Helvetica, sans-serif;
padding: .5em 2em .55em;
text-shadow: 0 1px 1px rgba(0,0,0,.3);
border-radius: .5em;
box-shadow: 0 1px 2px rgba(0,0,0,.2);
}